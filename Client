local Alias = Commands[i].Alias
	Create(Commands[i].Name)
	for v = 1,#Alias do
		Create(Alias[v])
	end
	CreateCommand(UDim2.new(0.0150422715,0,0.0127776451,0 + (i * 20)),Commands[i].Name.." "..Commands[i].Help)
end
